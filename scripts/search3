select distinct ea.pk.baseEntity  from EntityAttribute ea ,EntityAttribute ea0,EntityAttribute ea1,EntityAttribute ea2,EntityAttribute ea3 where  ea.pk.baseEntity.realm in (:realms)   and (ea0.pk.baseEntity.id=ea.pk.baseEntity.id and ea0.pk.attribute.code='PRI_LASTNAME'  and ea0.valueString LIKE '%CROW%'  and ea1.pk.baseEntity.id=ea.pk.baseEntity.id and ea1.pk.attribute.code='PRI_LASTNAME'  and ea2.pk.baseEntity.id=ea.pk.baseEntity.id and ea2.pk.attribute.code='PRI_FIRSTNAME'  ea3.pk.baseEntity.id=ea.pk.baseEntity.id and ea3.pk.attribute.code='PRI_DOB'  and ea3.valueDate < '1940-01-01')   order by ea1.valueString ASC,ea2.valueString ASC

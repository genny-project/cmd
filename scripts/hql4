select distinct ea.pk.baseEntity  from life.genny.qwanda.attribute.EntityAttribute ea, life.genny.qwanda.attribute.EntityAttribute ea0 , life.genny.qwanda.attribute.EntityAttribute ea1 where  ea.pk.baseEntity.realm = 'genny'   and ( ea0.pk.baseEntity.id=ea.pk.baseEntity.id and ea0.pk.attribute.code='PRI_LASTNAME'  and ea1.pk.baseEntity.id=ea.pk.baseEntity.id and ea1.pk.attribute.code='PRI_FIRSTNAME' and bitwise_and(ea0.valueLong , 256)  <> 0)   order by ea1.valueString ASC,ea0.valueString DESC

select distinct ea.pk.baseEntity  from life.genny.qwanda.attribute.EntityAttribute ea  where  ea.pk.baseEntity.realm in (:realms)   and ( ea0.pk.baseEntity.id=ea.pk.baseEntity.id and ea0.pk.attribute.code='PRI_LASTNAME'  and ea1.pk.baseEntity.id=ea.pk.baseEntity.id and ea1.pk.attribute.code='PRI_FIRSTNAME' )   order by ea1.valueString ASC,ea0.valueString DESC

select distinct ea.pk.baseEntity from EntityAttribute ea  ,EntityEntity ff  where  ea.pk.baseEntity.realm in ('genny')   and ea.pk.baseEntity.code LIKE 'BEG_%'  and ((ff.pk.targetCode='PER_ADAMCROW63_AT_GMAILCOM' and ff.pk.source.code=ea.pk.baseEntity.code) or (ff.pk.source.code='PER_ADAMCROW63_AT_GMAILCOM'  and ff.pk.targetCode=ea.pk.baseEntity.code)  )  order by  ea.pk.baseEntity.created DESC
